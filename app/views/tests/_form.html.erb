<%= form_for @test do |f| %>

  <%= f.label :name, "Test Name" %>
  <%= f.select :name, options_for_select(["GRE - Graduate Record Exam", "GMAT - Graduate Management Admission Test", "LSAT - Law School Admissions Test", "MCAT - Medical College Admissions Test", "TOEFL - Test of English as a Foreign Language"]), include_blank: true %>

  <br></br>

  <%= f.label :date, "Test Date" %>
  <%= f.date_field :date %>

  <br></br>

  <%= f.submit%>

<% end %>