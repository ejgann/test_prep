<%= form_for @practice do |f| %>

    <%= f.label :date %>
    <%= f.date_field :date %>
    <br></br>
    <%= f.label :rating %>
    <%= f.number_field :rating, min: 0, max: 5 %>
    <br></br>
    <%= f.label :activity_id, "Select an Activity"%>
    <%= f.select :activity_id, options_for_select(["Flash Cards", "Prep Course", "Tutoring", "Study Guide", "Group Study"]), include_blank: true %>
    <br></br>
    <%= f.label :comment %>
    <%= f.text_area :comment %>
    <br></br>
    <%= f.submit%>

<% end %>